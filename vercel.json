{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "installCommand": "npm install",
  "functions": {
    "api/upload-image.ts": {
      "maxDuration": 30
    },
    "api/tickets.ts": {
      "maxDuration": 10
    }
  }
} 